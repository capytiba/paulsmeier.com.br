-- gravity block --
-- by david meier --

-- player --
p = {
	x = 11*8,
	y = 3*8,
	xspd=0,
	yspd=0,
	sprite=4,
	invert=1,
	state=0
}
-- balloon --
b = {
	x = 5*8,
	y = 2*8,
	xspd=0,
	yspd=0,
	sprite=36,
	invert=-1,
	state=0
}
g = {
	x = 0,
	y = 0
}
-- rain --
rain={}
raindir = 4
rainsize = 2

wait = 0
mapx = 0
mapy = 0
lvl = 0

glbgx=0
glbgy=rainsize
lvlcoord = {
	23*8, 9*8,
	39*8, 7*8,
	55*8, 9*8
}

function _init()
	crtrain()
end

function _update()
	checkarrows()
	--move(b)
	b.state=0
	move(p)
	sound()
	updtrain()
	
	
	if finished(p) then
		wait+=1
	end
-- 	if wait >= 30*1 or btn(ğŸ…¾ï¸) then
	if wait>=30*1 then
		lvl+=1
		p.x = lvlcoord[lvl*2-1]
		p.y = lvlcoord[lvl*2]
		mapx = 16*lvl
		mapy = 16*flr(lvl/8)
		wait = 0
		dltrain()
		crtrain()
		if lvl==2 then p.sprite=2 end
	end
end

function _draw()
	cls()
	rectfill(0,0,127,127,-4)
	drwrain()
	map(mapx,mapy,0,0,16,16)

	spr(p.sprite,p.x-mapx*8,p.y-mapy*8)
	--spr(b.sprite,b.x-mapx*8,b.y-mapy*8)

	--print(mget((p.x+g.x)/8,(p.y+g.y)/8))
	--print((p.x)/8)
	--print((p.y)/8)
	--print(p.state)
	--print(b.state)
	--print(wait)
	--print(g.x)
	--print(g.y)

	if	finished(p) then
		print("you won",128/2-13,128/2)
	end
	if lvl == 3 then
		print("no more game for now", 128/2-40, 128/2)
		print("tell me if you finnished", 128/2-25*2, 128/2+7)
	end
end

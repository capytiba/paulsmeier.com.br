-- colide --
function colide(o)
	
	local x1=o.x/8
	local y1=o.y/8
	local x2=(o.x+7)/8
	local y2=(o.y+7)/8
	
	local a=fget(mget(x1,y1),0)
	local b=fget(mget(x1,y2),0)
	local c=fget(mget(x2,y1),0)
	local d=fget(mget(x2,y2),0)
	
	if a or b or c or d then
		if abs(g.x)+abs(g.y) > 0.5 then
			sfx(rnd(3))
		end
		return true
	else
		return false
	end
end

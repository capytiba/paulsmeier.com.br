-- movement --

function move(o)
	
	o.x += o.xspd
	o.y += o.yspd
	if o.state>0 and o.state<5 then
		o.xspd += g.x*o.invert
		o.yspd += g.y*o.invert
	end
	
	if o.xspd > 7 then o.xspd = 7 end
	if o.yspd > 7 then o.yspd = 7 end
	if o.xspd <-7 then o.xspd =-7 end
	if o.yspd <-7 then o.yspd =-7 end
	
	if colide(o) then
		if o.state==1 then
			o.x = (flr((o.x+7)/8)-1)*8
		end
		if o.state==2 then
			o.y = (flr((o.y)/8)+1)*8
		end
		if o.state==3 then
			o.x = (flr((o.x)/8)+1)*8
		end
		if o.state==4 then
			o.y = (flr((o.y+7)/8)-1)*8
		end
		o.xspd = 0
		o.yspd = 0
		o.state = 0
	end
end


function checkarrows(o)
	if p.state==0 and b.state==0 then
		g.x=0 -- resets gravity if
		g.y=0 -- if no btn pressed
		if btn(â¡ï¸) then
			g.x = 1
			g.y = 0
			p.state = 1
			b.state = 3
			glbgx=rainsize
			glbgy=0
		end
		if btn(â¬†ï¸) then
			g.y = -1
			g.x = 0
			p.state = 2
			b.state = 4
			glbgy=-rainsize
			glbgx=0
		end
		if btn(â¬…ï¸) then
			g.x = -1
			g.y = 0 
			p.state = 3
			b.state = 1
			glbgx=-rainsize
			glbgy=0
		end
		if btn(â¬‡ï¸) then
			g.y = 1
			g.x = 0
			p.state = 4
			b.state = 2
			glbgy=rainsize
			glbgx=0
		end
	end
end


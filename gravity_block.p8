-- rain --

-- create rain --
function crtrain()
	rainclr={13,5,1}
	rainspeed = 1
	for clr in all(rainclr) do
		for _ = 1,20*flr(sqrt(clr)) do
			add(rain,{
				x=-1+rnd(128),
				y=-1+rnd(128),
				c=clr,
				l=15,
				spd=rainspeed+rnd(rainspeed)
			})
		end
		rainspeed+=1
	end
end

-- update rain --
function updtrain()
	if p.state>0 and p.state<5 then
		raindir=p.state
	end
	
	for raindrop in all(rain) do
		if raindir==1 then raindrop.x+=raindrop.spd end
		if raindir==2 then raindrop.y-=raindrop.spd end
		if raindir==3 then raindrop.x-=raindrop.spd end
		if raindir==4 then raindrop.y+=raindrop.spd end
		if raindrop.x > 128 then raindrop.x=-1 end
		if raindrop.x < -1  then raindrop.x=128 end
		if raindrop.y > 128 then raindrop.y=-1 end
		if raindrop.y < -1  then raindrop.y=128 end
		
		--if raindrop.l<0 then del(rain,raindrop) end
		raindrop.l-=1
	end
end

-- delete rain --
function dltrain()
	for raindrop in all(rain) do
		del(rain,raindrop)
	end
end



-- draw rain / rd=raindrop --
function drwrain()
	for rd in all(rain) do
		line(rd.x,rd.y,rd.x+glbgx,rd.y+glbgy,rd.c)
	end
end
